@page
@model WebEmptyApp.Pages.StudentsModel
@{
}

<div class="row">
    <h2>Students</h2>

    <table class="table">
        <thead class="table-dark">
            <tr><th>Coding Factory Students</th></tr>
        </thead>
        <tbody>
            @if (Model.Students != null)
            {
                @foreach (var name in Model.Students)
                {
                    <tr><td>@name</td></tr>
                }
            }
        </tbody>
    </table>

</div>